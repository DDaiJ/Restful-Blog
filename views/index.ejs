<% include ./partials/header %>

<div class="frontspace"></div>

<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %></h2>
        <img class="ui big image" src="<%= blog.image %>">
        <span><%= blog.created%></span>
        <p><%= blog.body%></p>
        <a href="/blogs/<%= blog._id %>">Read More</a>
    </div>

<% }) %>

<% include ./partials/footer %>